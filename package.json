{
  "name": "react-video-streaming",
  "version": "1.0.0",
  "description": "A HLS video streaming application usin React,Express,FFmpeg & MongoDB",
  "main": "index.js",
  "scripts": {
    "install": "run-s install:**",
    "install:server": "cd server && npm install",
    "install:app": "cd client && npm install",
    "start": "run-s start:**",
    "start:server": "cd server && npm run start",
    "start:app": "cd client && npm run start",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "postinstall": "chmod -R +x ~/vendor/ffmpeg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grrbm/react-video-streaming.git"
  },
  "keywords": [],
