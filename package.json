  "bugs": {
    "url": "https://github.com/grrbm/react-video-streaming/issues"
  },
  "homepage": "https://github.com/grrbm/react-video-streaming#readme",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
